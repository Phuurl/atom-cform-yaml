'.source.yaml':
  'elastic-load-balancer':
    'prefix': 'elastic-load-balancer'
    'body': """
	${1:elb}:
		Type: AWS::ElasticLoadBalancing::LoadBalancer
		Properties:
			AvailabilityZones:
				- ${2:az}
			Listeners:
				- ${3:-}
			Instances:
				- ${4:-}
			SecurityGroups:
				- ${5:-}
			Subnets:
				- ${6:-}
			Tags:

    """
