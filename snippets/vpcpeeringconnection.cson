'.source.yaml':
  'vpcpeeringconnection':
      'prefix': 'vpcpeeringconnection'
      'body':"""
          ${1:vpcpeeringconnection_name}:
              Type: "AWS::EC2::VPCPeeringConnection"
              Properties:
                  PeerVpcId: ${2:String}     # required
                  Tags:                # optional, list of Tag
                      - ${3:Tag}
                  VpcId: ${4:String}     # required
"""