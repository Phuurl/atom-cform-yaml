'.source.yaml':
  'route':
    'prefix': 'route'
    'body': """
	${1:route_name}:
		Type: "AWS::EC2::Route"
		Properties:
			DestinationCidrBlock: ${2:0.0.0.0/0}
			GatewayId: ${3:gw_id}
			InstanceId: ${4:instance_id}
			NatGatewayId: ${5:nat_gw_id}
			NetworkInterfaceId: ${6:-}
			RouteTableId: ${7:-}
			VpcPeeringConnectionId: ${8:-}

    """
