'.source.yaml':
  'ec2-volume':
    'prefix': 'ec2-volume'
    'body': """
	${1:volume}:
		Type: AWS::EC2::Volume
		Properties:
			AutoEnableIO: ${2:'false'}
			AvailabilityZone: ${3:-}
			Encrypted: ${4:'false'}
			KmsKeyId: ${5:-}
			Size: ${6:-}
			SnapshotId: ${7:-}
			VolumeType: ${8:standard|io1|gp2}
			Tags:

    """
