'.source.yaml':
  'sqs-queue':
    'prefix': 'sqs-queue'
    'body': """
	${1:queueName}:
		Type: AWS::SQS::Queue
		Properties:
			DelaySeconds: ${2:1}
			MaximumMessageSize: ${3:128}
			MessageRetentionPeriod: ${4:1}
			QueueName: ${5:queueName}
			ReceiveMessageWaitTimeSeconds: ${6:5}
			RedrivePolicy: ${7:-}
			VisibilityTimeout: ${8:30}
    """
