'.source.yaml':
  'rds-dbsecuritygroup':
    'prefix': 'rds-dbsecuritygroup'
    'body': """
${1:securityGroup}:
	Type: AWS::RDS::DBSecurityGroup
	Properties:
		EC2VpcId: ${2:vpcRef}
		DBSecurityGroupIngress:
			- ${3:rds-dbsecuritygroupingress}
		GroupDescription: ${4:description}
		Tags:
			- ${5:tag}
    """
