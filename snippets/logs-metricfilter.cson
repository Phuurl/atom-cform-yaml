'.source.yaml':
  'logs-metricfilter':
    'prefix': 'logs-metricfilter'
    'body': """
${1:filter}:
	Type: AWS::Logs::MetricFilter
	Properties:
		FilterPattern: ${2:pattern}
		LogGroupName: ${3:name}
		MetricTransformations:
			- MetricValue: ${4:value}
			  MetricNamespace: ${5:namespace}
			  MetricName: ${6:name}
    """
