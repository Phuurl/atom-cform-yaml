'.source.yaml':
  'rds-eventsubscription':
    'prefix': 'rds-eventsubscription'
    'body': """
${1:subscription}:
	Type: AWS::RDS::EventSubscription
	Properties:
		Enabled: ${2:true}
		EventCategories:
			- ${3:category}
		SnstopicArn: ${4:topicRef}
		SourceIds:
			- ${5:sourceId}
		SourceType: ${6}
    """
