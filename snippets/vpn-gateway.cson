'.source.yaml':
  'vpn-gateway':
    'prefix': 'vpn-gateway'
    'body': """
	${1:vgwName}:
		Type: AWS::EC2::VPNGateway
		Properties:
			Type: ${2:ipsec.1}
			Tags:
				- ${3:-}
	AttachVpnGateway:
		Type: AWS::EC2::VPCGatewayAttachment
		Properties:
			VpcId: ${4:vpc}
			VpnGatewayId: 
				Ref: ${5:vgwName}
    """
